{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/configure/(.*)", "dest": "/index.js" },
    { "src": "^/configure$", "dest": "/index.js" },
    { "src": "^(.*)/manifest\\.json$", "dest": "/index.js" },
    { "src": "^(.*)/request_token$", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
